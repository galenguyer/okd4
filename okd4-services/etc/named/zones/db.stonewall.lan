$TTL    604800
@       IN      SOA     okd4-services.stonewall.lan. admin.stonewall.lan. (
                  1     ; Serial
             604800     ; Refresh
              86400     ; Retry
            2419200     ; Expire
             604800     ; Negative Cache TTL
)

; name servers - NS records
    IN      NS      okd4-services

; name servers - A records
okd4-services.stonewall.lan.          IN      A       192.168.1.200

; OpenShift Container Platform Cluster - A records
okd4-bootstrap.lab.stonewall.lan.        IN      A      192.168.1.210
okd4-master-1.lab.stonewall.lan.         IN      A      192.168.1.201
okd4-master-2.lab.stonewall.lan.         IN      A      192.168.1.202
okd4-master-3.lab.stonewall.lan.         IN      A      192.168.1.203
;okd4-compute-1.lab.stonewall.lan.        IN      A      192.168.1.204
;okd4-compute-2.lab.stonewall.lan.        IN      A      192.168.1.205

; OpenShift internal cluster IPs - A records
api.lab.stonewall.lan.    IN    A    192.168.1.200
api-int.lab.stonewall.lan.    IN    A    192.168.1.200
*.apps.lab.stonewall.lan.    IN    A    192.168.1.200
etcd-0.lab.stonewall.lan.    IN    A     192.168.1.201
etcd-1.lab.stonewall.lan.    IN    A     192.168.1.202
etcd-2.lab.stonewall.lan.    IN    A    192.168.1.203
console-openshift-console.apps.lab.stonewall.lan.     IN     A     192.168.1.210
oauth-openshift.apps.lab.stonewall.lan.     IN     A     192.168.1.210

; OpenShift internal cluster IPs - SRV records
_etcd-server-ssl._tcp.lab.stonewall.lan.    86400     IN    SRV     0    10    2380    etcd-0.lab
_etcd-server-ssl._tcp.lab.stonewall.lan.    86400     IN    SRV     0    10    2380    etcd-1.lab
_etcd-server-ssl._tcp.lab.stonewall.lan.    86400     IN    SRV     0    10    2380    etcd-2.lab
